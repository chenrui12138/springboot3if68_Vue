} s \n */\nexport function isNumber(s) {\n  return /(^-?[+-]?([0-9]*\\.?[0-9]+|[0-9]+\\.?[0-9]*)([eE][+-]?[0-9]+)?$)|(^$)/.test(s);\n}\n/**\n * 匹配整数，可以为空\n * @param {*} s \n */\nexport function isIntNumer(s) {\n  return /(^-?\\d+$)|(^$)/.test(s);\n}\n/**\n * 身份证校验\n */\nexport function checkIdCard(idcard) {\n  const regIdCard = /(^\\d{15}$)|(^\\d{18}$)|(^\\d{17}(\\d|X|x)$)/;\n  if (!regIdCard.test(idcard)) {\n    return false;\n  } else {\n    return true;\n  }\n}\n"]}]}