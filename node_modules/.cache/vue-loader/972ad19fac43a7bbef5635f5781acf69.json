ruleForm.mima && \"yonghu\" == this.flag) {\r\n        this.$message.error(\"密码不能为空\");\r\n        return;\r\n      }\r\n      if (\r\n        \"yonghu\" == this.flag &&\r\n        this.ruleForm.lianxidianhua &&\r\n        !isMobile(this.ruleForm.lianxidianhua)\r\n      ) {\r\n        this.$message.error(`联系电话应输入手机格式`);\r\n        return;\r\n      }\r\n      if (\"users\" == this.flag && this.ruleForm.xingming.trim().length < 1) {\r\n        this.$message.error(`用户名不能为空`);\r\n        return;\r\n      }\r\n      this.$http({\r\n        url: `${this.$storage.get(\"sessionTable\")}/update`,\r\n        method: \"post\",\r\n        data: this.ruleForm,\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          this.$message({\r\n            message: \"修改信息成功\",\r\n            type: \"success\",\r\n            duration: 1500,\r\n            onClose: () => {},\r\n          });\r\n        } else {\r\n          this.$message.error(data.msg);\r\n        }\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}